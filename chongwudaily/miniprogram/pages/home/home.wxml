<!-- miniprogram/pages/home/home.wxml -->
<view class="background">
    <view class="function-pick">
        <view class="pickone">
            <view class="one_icon">
                <view bindtap="toyimiao">
                    <image src="../../img/yimiao.png" class="yimiao_img"></image>
                </view>
                <view bindtap="toquchong">
                    <image src="../../img/quchong.png" class="quchong_img"></image>
                </view>
                <view bindtap="totijian">
                    <image src="../../img/tijian.png" class="tijian_img"></image>
                </view>
            </view>
            <view class="one_text">
                <view style="font-size:14px;" bindtap="toyimiao">疫 苗</view>
                <view style="font-size:14px;" bindtap="toquchong">驱 虫</view>
                <view style="font-size:14px;" bindtap="totijian">体 检</view>
            </view>
        </view>
        <view class="picktwo">
            <view class="two_icon">
                <view bindtap="toyaoyu">
                    <image src="../../img/yaoyu.png" class="yaoyu_img"></image>
                </view>
                <view bindtap="tomeirong">
                    <image src="../../img/meirong.png" class="meirong_img"></image>
                </view>
                <view bindtap="toxizao">
                    <image src="../../img/xizao.png" class="xizao_img"></image>
                </view>
            </view>
            <view class="two_text">
                <view style="font-size:14px;" bindtap="toyaoyu">药 浴</view>
                <view style="font-size:14px;" bindtap="tomeirong">美 容</view>
                <view style="font-size:14px;" bindtap="toxizao">洗 澡</view>
            </view>
        </view>
    </view>
    <view style="height:6vh;"></view>
    <view class="fenge"></view>
    <view class="pedia">
        <view class="mao">
            <view class="maoke"></view>
            <view class="maoke"></view>
        </view>
        <view class="gou">
            <view class="gouke"></view>
            <view class="gouke"></view>
        </view>
    </view>
</view>